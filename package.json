{
    "name": "ccl-stack",
    "private": true,
    "description": "",
    "license": "",
    "sideEffects": false,
    "scripts": {
        "postinstall": "remix setup node",
        "build": "run-s build:*",
        "build:css": "postcss styles --base styles --dir app/styles --env production",
        "build:remix": "remix build",
        "dev": "run-p dev:*",
        "dev:css": "postcss styles --base styles --dir app/styles && postcss styles --base styles --dir app/styles -w",
        "dev:remix": "cross-env NODE_ENV=development remix dev",
        "dev-mock-api": "run-p dev-mock-api:*",
        "dev-mock-api:css": "postcss styles --base styles --dir app/styles && postcss styles --base styles --dir app/styles -w",
        "dev-mock-api:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
        "format": "prettier --write .",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
        "start": "remix-serve build",
        "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
        "test": "vitest",
        "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
        "pretest:e2e:run": "npm run build",
        "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
        "typecheck": "tsc -b && tsc -b cypress",
        "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
    },
    "eslintIgnore": [
        "node_modules",
        "coverage",
        "server",
        "build",
        "public/build",
        "*.ignored/",
        "*.ignored.*",
        "public/script/axe.min.js"
    ],
    "dependencies": {
        "@remix-run/node": "*",
        "@remix-run/react": "*",
        "@remix-run/serve": "*",
        "@remix-run/server-runtime": "*",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tiny-invariant": "^1.2.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^6.3.1",
        "@remix-run/dev": "*",
        "@remix-run/eslint-config": "*",
        "@testing-library/cypress": "^8.0.3",
        "@testing-library/dom": "^8.14.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^14.2.1",
        "@types/eslint": "^8.4.3",
        "@types/node": "^18.0.0",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@vitejs/plugin-react": "^1.3.2",
        "autoprefixer": "^10.4.7",
        "binode": "^1.0.5",
        "c8": "^7.11.3",
        "cross-env": "^7.0.3",
        "cssnano": "^5.1.12",
        "cypress": "^10.0.3",
        "dotenv": "^16.0.1",
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.5.0",
        "happy-dom": "^5.3.1",
        "msw": "^0.42.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.12",
        "postcss-cli": "^9.1.0",
        "postcss-loader": "^7.0.0",
        "postcss-preset-env": "^7.7.1",
        "prettier": "2.7.1",
        "start-server-and-test": "^1.14.0",
        "ts-node": "^10.8.1",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^4.7.4",
        "vite": "^2.9.12",
        "vite-tsconfig-paths": "^3.5.0",
        "vitest": "^0.15.2"
    },
    "engines": {
        "node": ">=16"
    }
}
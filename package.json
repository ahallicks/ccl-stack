{
    "name": "ccl-stack",
    "private": true,
    "description": "",
    "license": "",
    "sideEffects": false,
    "scripts": {
        "postinstall": "remix setup node",
        "build": "run-s build:*",
        "build:css": "postcss styles --base styles --dir app/styles --env production",
        "build:remix": "remix build",
        "dev": "run-p dev:*",
        "dev:css": "npm run generate:css -- --watch",
        "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
        "format": "prettier --write .",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
        "setup": "prisma migrate dev && prisma db seed",
        "start": "remix-serve build",
        "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
        "test": "vitest",
        "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"cypress open\"",
        "pretest:e2e:run": "npm run build",
        "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"cypress run\"",
        "typecheck": "tsc -b && tsc -b cypress",
        "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
    },
    "eslintIgnore": [
        "node_modules",
        "coverage",
        "server",
        "build",
        "public/build",
        "*.ignored/",
        "*.ignored.*",
        "public/script/axe.min.js"
    ],
    "dependencies": {
        "@prisma/client": "^3.13.0",
        "@remix-run/node": "*",
        "@remix-run/react": "*",
        "@remix-run/serve": "*",
        "@remix-run/server-runtime": "*",
        "bcryptjs": "^2.4.3",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "tiny-invariant": "^1.2.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^6.2.0",
        "@remix-run/dev": "*",
        "@remix-run/eslint-config": "*",
        "@testing-library/cypress": "^8.0.2",
        "@testing-library/dom": "^8.13.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.1.1",
        "@testing-library/user-event": "^14.1.1",
        "@types/bcryptjs": "^2.4.2",
        "@types/eslint": "^8.4.1",
        "@types/node": "^17.0.30",
        "@types/react": "^18.0.8",
        "@types/react-dom": "^18.0.3",
        "@vitejs/plugin-react": "^1.3.1",
        "autoprefixer": "^10.4.4",
        "binode": "^1.0.5",
        "c8": "^7.11.2",
        "cross-env": "^7.0.3",
        "cssnano": "^5.1.7",
        "cypress": "^9.6.0",
        "dotenv": "^16.0.0",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "happy-dom": "^3.1.0",
        "msw": "^0.39.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.12",
        "postcss-cli": "^9.1.0",
        "postcss-loader": "^6.2.1",
        "postcss-preset-env": "^7.4.4",
        "prettier": "2.6.2",
        "prisma": "^3.13.0",
        "start-server-and-test": "^1.14.0",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^3.14.1",
        "typescript": "^4.6.4",
        "vite": "^2.9.6",
        "vite-tsconfig-paths": "^3.4.1",
        "vitest": "^0.10.0"
    },
    "engines": {
        "node": ">=16"
    },
    "prisma": {
        "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
    }
}